# ──────────────────────────────────────────────────────────
# .env.example — Copy this file to .env and fill in your values.
# NEVER commit your real .env file to version control.
# ──────────────────────────────────────────────────────────

# Your Chat Thing API secret key
# Found in: Chat Thing dashboard → Settings → API
CHATTHING_API_KEY=your_api_secret_key_here

# The public channel ID you want to connect to
# Found in: Chat Thing dashboard → Channels → <your channel> → Channel ID
CHATTHING_CHANNEL_ID=your_channel_id_here

# Port the local server listens on (default: 3000)
PORT=3000
